  "main": "./dist/server.js",
  "scripts": {
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess 'node ./dist/server.js'",
    "tsc": "tsc",
    "prettier": "npx prettier --write ./src/*/**.ts && npx prettier --write ./src/**.ts",
    "lint": "eslint ./src/*/**.ts && eslint ./src/**.ts",
    "start": "dir && npm install && node .",
    "build": "npx tsc",
    "jasmine":  "eb setenv ENV=test&&db-migrate --env test up&&db-migrate --env test down&&jasmine&&db-migrate db:drop test&&eb setenv ENV=dev",
    "test": "npm run build && npm run jasmine",
    "deploy": "chmod +x ./bin/deploy.sh && ./bin/deploy.sh"
  },
